{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:migrate && cd server && NODE_ENV=test mocha  --timeout 15000 --compilers js:babel-core/register ./tests/ --recursive --exit",
    "build": "babel ./server -d ./build/server && cd build/server && node index.js",
    "start": "cd server && NODE_ENV=production babel-node index",
    "dev": "cd server && NODE_ENV=development nodemon --exec babel-node index",
    "coverage": "cross-env && cd server  &&  NODE_ENV=test nyc _mocha  --timeout 15000 --compilers js:babel-core/register ./tests/ --recursive --exit && nyc report --reporter=text-lcov | coveralls",
    "report": "npm run coverage &&  codeclimate-test-reporter < ./coverage/lcov.info",
    "report:travis": " npm run coverage && codeclimate-test-reporter < ./coverage/lcov.info",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "test:migrate": "NODE_ENV=test sequelize db:migrate",
    "migrate": "sequelize db:migrate"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
